<p id="notice"><%= notice %></p>

<h1>Userteams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userteams.each do |userteam| %>
      <tr>
        <td><%= link_to 'Show', userteam %></td>
        <td><%= link_to 'Edit', edit_userteam_path(userteam) %></td>
        <td><%= link_to 'Destroy', userteam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userteam', new_userteam_path %>
