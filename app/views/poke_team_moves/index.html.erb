<p id="notice"><%= notice %></p>

<h1>Poke Team Moves</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poke_team_moves.each do |poke_team_move| %>
      <tr>
        <td><%= link_to 'Show', poke_team_move %></td>
        <td><%= link_to 'Edit', edit_poke_team_move_path(poke_team_move) %></td>
        <td><%= link_to 'Destroy', poke_team_move, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poke Team Move', new_poke_team_move_path %>
